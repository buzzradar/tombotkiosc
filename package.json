    "serve": "^11.3.2",
    "uglify-js": "^2.6.1",
    "uglifyify": "^3.0.1",
    "watchify": "^3.7.0"
  },
  "dependencies": {
    "babelify": "^6.0.1",
    "bowser": "^1.0.0",
    "d3": "^3.5.16",
    "eventemitter3": "^1.1.1",
    "glslify": "^5.0.2",
    "gulp-html-js-template": "^0.1.6",
    "gulp-statil": "0.0.9",
    "handlebars": "^4.0.5",
    "jquery": "^2.2.0",
    "lodash": "^4.0.0"
  }
}
