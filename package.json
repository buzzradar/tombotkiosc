{
  "name": "BuzzRadar-TomBot-Kiosc",
  "version": "1.0.0",
  "description": "Tombot Electron application",
  "main": "main.js",
  "scripts": {
    "watch": "watchify app/src/main.js -v -o app/js/tombot.js & ",
    "dev": "npm run watch && gulp watch",
    "electron": "electron --inspect=5858 .",
    "build": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=app/assets/icon/mac/tombot_app_icon.icns --prune=true --out=release-builds"
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "~1.7.8",
    "electron-packager": "^9.1.0",
    "browser-sync": "*",
    "browserify": "^13.0.0",
    "eslint": "^1.1.1",
    "gulp": "^3.9.0",
    "gulp-compass": "*",
    "gulp-concat": "^2.6.0",
    "gulp-css-to-js": "0.0.1",
    "gulp-csstojs": "^1.0.1",
    "gulp-dest": "^0.2.3",
    "gulp-load-plugins": "*",
    "gulp-merge": "^0.1.1",
    "gulp-plumber": "^1.1.0",
    "gulp-rename": "^1.2.2",
    "gulp-ruby-sass": "*",
    "gulp-uglify": "^2.1.2",
